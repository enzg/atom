{
  "name": "panda",
  "profile": "ray",
  "regions": [
    "ap-southeast-1"
  ],
  "hooks": {
    "build": "make"
  },
  "proxy": {
    "command": "./node-v9.7.0-linux-x64/bin/node ./panda/es7.js",
    "timeout": 25
  },
  "lambda": {
    "memory": 512
  },
  "cors": {
    "allowed_origins": [
      "*"
    ],
    "allowed_methods": [
      "HEAD",
      "GET",
      "POST",
      "PUT",
      "PATCH",
      "OPTIONS"
    ],
    "allowed_headers": [
      "Content-Type",
      "Authorization"
    ]
  }
}